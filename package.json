{
  "name": "streamz",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\" --turbopack",
    "dev:backend": "bun --watch src/app/api/server.ts",
    "dev:frontend": "next dev --turbopack",
    "build": "bun run build:backend && next build --turbopack",
    "build:backend": "bun build src/app/api/server.ts --outdir dist --target node",
    "start": "concurrently \"bun dist/server.ts\" \"next start\"",
    "start:backend": "bun dist/server.ts",
    "main": "bun dist/server.ts",
    "start:frontend": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.53.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.31.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "bcrypt",
    "esbuild",
    "sharp",
    "unrs-resolver"
  ],
  "keywords": [
    "video",
    "streaming",
    "frontend",
    "streamz",
    "movies",
    "series"
  ],
  "author": "(jaxxx)",
  "license": "MIT"
}
